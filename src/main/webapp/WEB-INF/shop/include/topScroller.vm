<!-- 二维码栏 -->
<div class="row col-xs-12">
    <div class="panel panel-default">
        <div class="panel-heading">
            <div class="row">
                <div class="col-lg-3" style="height: 250px;">
                    <div id="qrcodeCarousel" class="carousel slide pad_010 b_k" data-ride="carousel" style="height: 250px;">
                        <!-- 轮播（Carousel）指标 -->
                        <ol class="carousel-indicators">
                            #set( $partner_count = 0 )
                            #foreach($partner in $partnerList)
                                #if($!velocityCount == 1)
                                    <li data-target="#qrcodeCarousel" data-slide-to="$!{partner_count}" class="active"></li>
                                #else
                                    <li data-target="#qrcodeCarousel" data-slide-to="$!{partner_count}"></li>
                                #end
                                #set($partner_count = $partner_count+1)
                            #end
                        </ol>
                        <!-- 轮播（Carousel）项目 -->
                        <div class="carousel-inner bor_btm" style="height: 250px;">
                            #foreach($partner in $partnerList)
                                #if($!velocityCount == 1)
                                    <div class="item active" style="height: 250px;">
                                        <div class="col-sm-12 col-lg-12">
                                            <p class="shop-margin-left-30"><font color="red" style="font-size: 20px;">北&nbsp;&nbsp;&nbsp;&nbsp;翼&nbsp;&nbsp;&nbsp;&nbsp;商&nbsp;&nbsp;&nbsp;&nbsp;铺</font></p>
                                            <p class="shop-margin-left-30" style="margin-top:-15px;"><font color="red" style="font-size: 20px;">创&nbsp;&nbsp;&nbsp;&nbsp;造&nbsp;&nbsp;&nbsp;&nbsp;财&nbsp;&nbsp;&nbsp;&nbsp;富</font></p>
                                        </div>
                                        <div class="col-sm-12 col-lg-12" style="margin-top:-5px;">
                                            <img  class="shop-margin-left-30" src="/static/ui/images/qrcode.png" style="height: 160px;">
                                        </div>
                                        <div class="col-sm-12 col-lg-12 shop-margin-left-30">
                                            <span style="font-size: 18px;"><font>北&nbsp;&nbsp;&nbsp;翼&nbsp;&nbsp;&nbsp;商&nbsp;&nbsp;&nbsp;&nbsp;铺&nbsp;&nbsp;&nbsp;&nbsp;网</font></span>
                                        </div>
                                    </div>
                                #else
                                    <div class="item" >
                                        <div class="col-sm-12 col-lg-12">
                                            <img src="$!partner.fileName" style="height: 300px;" style="height: 250px;">
                                        </div>
                                    </div>
                                #end
                            #end
                        </div>
                        <!-- 轮播（Carousel）导航 -->
                        <a class="carousel-control left" href="#qrcodeCarousel" data-slide="prev">&lsaquo;</a>
                        <a class="carousel-control right" href="#qrcodeCarousel" data-slide="next">&rsaquo;</a>
                    </div>

                </div>
